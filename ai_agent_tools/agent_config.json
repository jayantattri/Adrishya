      "temperature": 0.2,
      "max_tokens": 1200,
      "max_tool_calls": 8,
      "timeout": 90,
      "personality": "privacy-conscious and reliable",
      "capabilities": [
        "navigation",
        "basic_automation",
        "privacy_protection",
        "local_processing"
