      "max_tokens": 1500,
      "max_tool_calls": 10,
      "timeout": 30,
      "personality": "helpful and efficient",
      "capabilities": [
        "navigation",
        "tab_management", 
        "page_interaction",
        "form_filling",
        "information_extraction",
        "automation"
      ]
    },
    "research_assistant": {
      "name": "Research Assistant",
